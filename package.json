{
  "name": "free-format-input",
  "displayName": "Free Format Input",
  "description": "Syntax highlight for free format input",
  "author": {
    "name": "Keunhee Cho"
  },
  "publisher": "Life-Science",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "license": "MIT (see LICENSE)",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "syntax highlight",
    "free format input"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kchoworks/free-format-input"
  },
  "contributes": {
    "languages": [
      {
        "id": "inp",
        "aliases": [
          "freeFormat",
          "inp"
        ],
        "extensions": [
          ".inp"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "inp",
        "scopeName": "source.inp",
        "path": "./syntaxes/inp.tmLanguage.json"
      }
    ]
  }
}
